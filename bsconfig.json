{
  "name": "reason-react-starter-kit",
  "version": "0.1.0",
  "sources": [
    {
      "dir": "src",
      "subdirs": true
    }
  ],
  "package-specs": [
    {
      "module": "commonjs"
    },
    {
      "module": "es6-global",
      "in-source": true
    }
  ],
  "suffix": ".bs.js",
  "bs-dependencies": [
    "@glennsl/bs-json",
    "bs-css",
    "bs-dynamic-import",
    "bs-express",
    "reason-react",
    "reason-apollo",
    "reason-loadable"
  ],
  "namespace": true,
  "refmt": 3,
  "reason": {
    "react-jsx": 2
  },
  "bsc-flags": ["-bs-super-errors"],
  "ppx-flags": ["graphql_ppx/ppx"]
}
