{
  "name": "reasonreact-ssr",
  "version": "0.1.0",
  "scripts": {
    "build:client": "webpack",
    "build:server": "webpack --config webpack.server.config.js",
    "build": "bsb -make-world && yarn build:client && yarn build:server",
    "start": "node dist/server",
    "predev": "yarn build",
    "dev": "concurrently \"bsb -make-world -w\" \"node dist/server\"",
    "clean": "bsb -clean-world",
    "watch": "bsb -make-world -w"
  },
  "license": "MIT",
  "dependencies": {
    "dotenv": "^5.0.0",
    "express": "^4.16.2",
    "helmet": "^3.11.0",
    "loadable-components": "^1.1.1",
    "morgan": "^1.9.0",
    "react": "^16.2.0",
    "react-apollo": "^2.0.4",
    "react-dom": "^16.2.0",
    "react-helmet-async": "^0.0.5",
    "react-loadable": "^5.3.1",
    "reason-react": "^0.3.2"
  },
  "devDependencies": {
    "bs-chalk": "^0.2.1",
    "bs-express": "^0.4.0",
    "bs-platform": "^2.2.1",
    "concurrently": "^3.5.1",
    "copy-webpack-plugin": "^4.4.1",
    "webpack": "^4.0.0-beta.2",
    "webpack-assets-manifest": "^1.0.0",
    "webpack-cli": "^2.0.6",
    "webpack-dev-middleware": "^2.0.5",
    "webpack-hot-middleware": "^2.21.0",
    "webpack-node-externals": "^1.6.0"
  }
}